concat_matricies <- function(..., id, names, .transform) {
mats <- as.list(...)
args <- lapply(1:length(mats), function(i) {
out <- .transform(mats[[i]]) %>%
as.data.frame() %>%
dplyr::mutate(estimator = id[i], .before = 1)
names(out) <- names
out
})
do.call(rbind, args)
}
