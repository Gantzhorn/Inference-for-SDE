optimizer <- function(par,
fn,
...
) {
tryCatch({
optim(
par = par,
fn = fn,
...,
control = list(
reltol = sqrt(.Machine$double.eps) / 1e8,
maxit = 1000L
),
method = "BFGS"
)$par
},
error = function(e) {
optim(
par = par,
fn = fn,
...,
control = list(
reltol = sqrt(.Machine$double.eps) / 1e8,
maxit = 1000L
)
)$par
}
)
}
